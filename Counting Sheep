using System;

public static class Kata
{
  public static int CountSheeps(bool[] sheeps)
  {
    int count=0;
    if(sheeps.GetType().GetElementType() == typeof(bool)){
      for(int i = 0; i < sheeps.Length;i++) {
       if(sheeps[i] == true){
        count++;
       }
       } return count;
      } else {
      return count;
    }
    } 
  }
